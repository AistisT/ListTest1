@model ListTest.Models.Test
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using (Html.BeginForm("Update", "Home", FormMethod.Post))
{
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
               Test
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <div class="dataTable_wrapper">
                    <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                        <thead>
                            <tr>
                                <th>ID</th>
                                <th>Editable Column</th>
                            </tr>
                        </thead>
                        <tbody>
                            @for (int i=0;  i<Model.list.Count(); i++)
                            {
                                <tr class="odd gradeX">
                                    <td>@(Model.list[i].id)</td>
                                    <td>@Html.EditorFor(model => model.list[i].value)</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
            <div id="btn" class="col-lg-6">
                <button type="submit" class="btn btn-default" value="Update">Update</button>
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>
}